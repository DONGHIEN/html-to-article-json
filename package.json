{
  "name": "content-editable-ng",
  "version": "0.0.0",
  "description": "A new take on contenteditable, virtual-dom style",
  "main": "dist/lib/index.js",
  "scripts": {
    "benchmark": "babel-node benchmark/node.js",
    "example": "beefy example/client.js --index example/index.html --open -- --transform babelify",
    "lint": "semistandard | snazzy",
    "build": "rm -rf dist && babel lib --out-dir dist",
    "prepublish": "npm run build",
    "postpublish": "rm -rf dist",
    "test": "npm run lint && npm prune && npm run test:node && npm run test:coverage",
    "test:coverage": "nyc babel-tape-runner test/*-test.js && nyc check-coverage --lines 100 --functions 100 --branches 100",
    "test:node": "babel-tape-runner test/*-test.js | tap-difflet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/micnews/content-editable-ng.git"
  },
  "author": "mic.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/micnews/content-editable-ng/issues"
  },
  "homepage": "https://github.com/micnews/content-editable-ng#readme",
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-preset-es2015": "^6.3.13",
    "babel-tape-runner": "^2.0.0",
    "babelify": "^7.2.0",
    "beefy": "^2.1.5",
    "keymaster": "^1.6.2",
    "nyc": "^5.3.0",
    "pre-commit": "^1.1.1",
    "semistandard": "^7.0.2",
    "snazzy": "^2.0.1",
    "tap-difflet": "^0.4.0",
    "tape": "^4.2.0",
    "tsml": "^1.0.1",
    "watchify": "^3.4.0",
    "xtend": "^4.0.0"
  },
  "dependencies": {
    "array-from": "^2.0.2",
    "block-elements": "^1.1.0",
    "dom-style-parser": "^1.0.0",
    "es6-set": "^0.1.2",
    "generate-function": "^2.0.0",
    "lodash.find": "^4.0.0",
    "parse5": "^2.1.3"
  },
  "pre-commit": [
    "test:coverage"
  ]
}
