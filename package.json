{
  "name": "content-editable-ng",
  "version": "0.0.0",
  "description": "A new take on contenteditable, virtual-dom style",
  "main": "dist/lib/index.js",
  "scripts": {
    "benchmark": "babel-node benchmark/node.js",
    "benchmark:browser": "beefy benchmark/browser.js --open -- --transform babelify --transform brfs",
    "example": "beefy example/client.js --index example/index.html --open -- --transform babelify",
    "lint": "semistandard | snazzy",
    "build": "rm -rf dist && babel lib --out-dir dist",
    "prepublish": "npm run build",
    "postpublish": "rm -rf lib",
    "test": "npm run lint && npm prune && npm run test:node && npm run test:electron && npm run test:coverage",
    "test:coverage": "browserify test/*-test.js --transform babelify --transform brfs --transform coverify | testron | coverify",
    "test:electron": "browserify test/*-test.js --transform babelify --transform brfs | testron | tap-difflet",
    "test:node": "babel-tape-runner test/utils/browser-runner.js test/*-test.js | tap-difflet",
    "test:zuul": "zuul --local 1337 -- ./test/*-test.js ./test/browser/*-test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/micnews/content-editable-ng.git"
  },
  "author": "mic.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/micnews/content-editable-ng/issues"
  },
  "homepage": "https://github.com/micnews/content-editable-ng#readme",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-tape-runner": "^1.2.0",
    "babelify": "^6.3.0",
    "beefy": "^2.1.5",
    "brfs": "^1.4.1",
    "browserify": "^11.2.0",
    "coverify": "^1.4.1",
    "electron-prebuilt": "^0.36.4",
    "jsdom": "^6.3.0",
    "keymaster": "^1.6.2",
    "pre-commit": "^1.1.1",
    "semistandard": "^7.0.2",
    "snazzy": "^2.0.1",
    "tap-difflet": "^0.4.0",
    "tape": "^4.2.0",
    "testron": "^1.2.0",
    "watchify": "^3.4.0",
    "xtend": "^4.0.0",
    "zuul": "^3.5.0"
  },
  "dependencies": {
    "array-from": "^2.0.2",
    "block-elements": "^1.1.0",
    "es6-set": "^0.1.2",
    "generate-function": "^2.0.0"
  },
  "pre-commit": [
    "test:coverage"
  ]
}
